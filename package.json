{
  "name": "toy-block-charts",
  "description": "Charts that looks like they were made out of toy blocks.",
  "repository": {
    "type": "git",
    "url": "https://github.com/motinados/toy-block-charts.git"
  },
  "version": "0.4.0",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "author": "motinados",
  "keywords": [
    "charts",
    "toy",
    "react"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
